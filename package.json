{
  "name": "luxe-framework",
  "version": "1.0.0",
  "description": "LuxeFramework - Build modular, solid and reliable web apps easily",
  "main": "dist/index.js",
  "author": "MaxRattle <quesesr@gmail.com>",
  "license": "MIT",
  "scripts": {
    "do": "tsc && node dist/src/console.js",
    "do:dbg": "tsc && cross-env DEBUGGER=true node --inspect-brk --nolazy dist/src/app/console.js",
    "do:prod": "cross-env NODE_ENV=production node dist/src/app/console.js",
    "start": "nodemon --config lib/run/nodemon.json --exec \"tsc && node dist/src/app/web.js\"",
    "start:dbg": "tsc && cross-env DEBUGGER=true node --inspect-brk --nolazy dist/src/app/web.js",
    "start:prod": "gulp build && cross-env NODE_ENV=production node dist/src/app/web.js",
    "build": "gulp build",
    "build:lib": "gulp clean && tsc -p tsconfig.build.json",
    "clean": "gulp clean",
    "test": "gulp test --only",
    "test:watch": "gulp test --watch --only",
    "test:dbg": "gulp test --dbg --only",
    "test:slow": "gulp test --slow",
    "lint:lib": "eslint lib/**/*.ts",
    "orm": "gulp orm --cmd",
    "orm:prod": "gulp orm --prod --cmd",
    "orm:test": "gulp orm --test --cmd"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.7",
    "@nestjs/core": "^7.0.7",
    "@nestjs/platform-express": "^7.0.7",
    "bcrypt": "^4.0.1",
    "cache-manager": "^3.2.1",
    "cache-manager-redis": "^0.6.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "mobile-detect": "^1.4.4",
    "mocha": "^7.1.1",
    "module-alias": "^2.2.2",
    "mysql2": "^2.1.0",
    "nodemon": "^2.0.3",
    "rate-limiter-flexible": "^2.1.3",
    "redis": "^3.0.2",
    "redlock": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "typeorm": "0.3.0-rc.9",
    "winston": "^3.2.1",
    "yargs": "^15.3.1",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cache-manager": "^2.10.2",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.4",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/mocha": "^7.0.2",
    "@types/node": "^10.0.0",
    "@types/redis": "^2.8.16",
    "@types/redlock": "^4.0.1",
    "@types/rimraf": "^3.0.0",
    "@types/sinon": "^9.0.0",
    "@types/yup": "^0.26.35",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "npm-run": "^5.0.1",
    "source-map-support": "^0.5.16",
    "sqlite3": "^4.1.1",
    "string-argv": "^0.3.1",
    "typescript": "^3.8.3"
  }
}
