{
  "name": "lyxe",
  "version": "1.2.3",
  "description": "LyxeFramework - Build modular, solid and reliable web apps easily",
  "main": "lib/core/LyxeFramework.js",
  "author": "Maxim Kobozev <quesesr@gmail.com>",
  "license": "MIT",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/queses/lyxe.js"
  },
  "keywords": [
    "ddd",
    "solid",
    "oop",
    "modular",
    "backend",
    "framework"
  ],
  "scripts": {
    "lyxe": "lyxe",
    "prepublish": "lyxe build-lib",
    "do": "lyxe do",
    "do:dbg": "lyxe do --debug true",
    "do:prod": "cross-env NODE_ENV=production node dist/src/app/console.js",
    "dev": "lyxe dev",
    "dbg": "lyxe dev --dbg",
    "prod": "lyxe build && cross-env NODE_ENV=production node dist/src/app/web.js",
    "build": "lyxe build",
    "build:lib": "lyxe build-lib",
    "clean": "lyxe clean",
    "test": "lyxe test",
    "test:watch": "lyxe test --watch --only",
    "test:dbg": "lyxe test --quiet --dbg --only",
    "test:slow": "lyxe test --slow",
    "lint:lib": "lyxe lintLib",
    "orm": "lyxe orm",
    "orm:prod": "lyxe orm --prod true",
    "orm:test": "lyxe orm --test true"
  },
  "bin": {
    "lyxe": "libsrc/run/cli.js"
  },
  "dependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^7.0.2",
    "@types/sinon": "^9.0.0",
    "@types/yup": "^0.26.35",
    "bcrypt": "^5.0.0",
    "cache-manager": "^3.2.1",
    "cache-manager-redis": "^0.6.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.2",
    "cross-spawn": "^7.0.3",
    "dotenv": "^8.2.0",
    "gulp": "^4.0.2",
    "inversify": "^5.0.1",
    "jake": "^10.8.2",
    "jsonwebtoken": "^8.5.1",
    "mobile-detect": "^1.4.4",
    "mocha": "^7.2.0",
    "module-alias": "^2.2.2",
    "mysql2": "^2.1.0",
    "nodemon": "^2.0.3",
    "rate-limiter-flexible": "^2.1.3",
    "redis": "^3.0.2",
    "redlock": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "source-map-support": "^0.5.16",
    "string-argv": "^0.3.1",
    "winston": "^3.2.1",
    "yargs": "^15.3.1",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@nestjs/common": "^7.0.7",
    "@nestjs/core": "^7.0.7",
    "@nestjs/platform-express": "^7.0.7",
    "@types/bcrypt": "^3.0.0",
    "@types/cache-manager": "^2.10.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.4",
    "@types/jake": "^0.0.32",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/node": "^10.0.0",
    "@types/redis": "^2.8.16",
    "@types/redlock": "^4.0.1",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "copyfiles": "^2.3.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "express": "^4.17.1",
    "sqlite3": "^4.1.1",
    "typeorm": "0.3.0-rc.9",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "@nestjs/common": ">= 7.0.0",
    "@nestjs/core": ">= 7.0.0",
    "@nestjs/platform-express": ">= 7.0.0",
    "typeorm": ">= 0.2.0"
  }
}
